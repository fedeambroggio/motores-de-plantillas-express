<% if (productosGet.length > 0) { %>

<table style="border: 1px solid black">
    <tr>
        <th style="border: 1px solid black; padding: 8px">Nombre</th>
        <th style="border: 1px solid black; padding: 8px">Precio</th>
        <th style="border: 1px solid black; padding: 8px">URL Imagen</th>
    </tr>
    <% productosGet.forEach(function(prod) { %>
    <tr>
        <td style="border: 1px solid black; padding: 8px"><%=prod.title%></td>
        <td style="border: 1px solid black; padding: 8px"><%=prod.price%></td>
        <td style="border: 1px solid black; padding: 8px">
            <img
                style="width: 64px; height: auto"
                src="<%=prod.thumbnail%>"
                alt="prod_img"
            />
        </td>
    </tr>
    <% }); %>
</table>
<%} else { %>
<p style="color: red; font-size: 20px">Sin productos para mostrar</p>
<% } %>
