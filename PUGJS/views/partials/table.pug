table(style="border: 1px solid black;")
  tr
    th(style="border: 1px solid black; padding: 8px") Nombre
    th(style="border: 1px solid black; padding: 8px") Precio
    th(style="border: 1px solid black; padding: 8px") URL Imagen
  each prod in productosGet
    tr
      th(style="border: 1px solid black; padding: 8px") #{prod.title}
      th(style="border: 1px solid black; padding: 8px") #{prod.price}
      th(style="border: 1px solid black; padding: 8px")
        img(style="width: 64px; height: auto", src=prod.thumbnail, alt="prod_img")
